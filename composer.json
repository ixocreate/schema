{
    "name": "ixocreate/schema",
    "description": "Schema implementation for IXOCREATE",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/schema-contract": "dev-develop",
        "ixocreate/entity": "dev-develop"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0.1",
        "phpunit/phpunit": "^6.0.6",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Schema\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateMisc\\Schema\\": "tests/misc/",
            "IxocreateTest\\Schema\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
